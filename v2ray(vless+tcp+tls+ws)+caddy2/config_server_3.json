{
  "log": {
    "error": "/var/log/v2ray/error.log",
    "access": "/var/log/v2ray/access.log",
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 443, // 监听端口
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "443@gmail.com",
            "id": "888fd3ac-dd53-11e9-b357-5600024c1d6a" //修改成自己的UUID
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 80,
            "xver": 0
          },
          {
            "path": "/SS1v2ray", //通过路径分流出ws
            "dest": 2004, //分流后转发到2004端口
            "xver": 0
          }
        ]
      },
      "streamSettings": {
        "security": "tls",
        "tlsSettings": {
          "alpn":[
            "h2",
            "http/1.1"
          ],
          "certificates": [
            {
              "certificateFile": "/etc/ssl/tls/xx.yy.crt",
              "keyFile": "/etc/ssl/tls/xx.yy.key"
            }
          ]
        },
        "network": "tcp"
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "tag": "ss1ws",
      "port": "2004", // 监听端口
      "listen": "127.0.0.1", //只监听 127.0.0.1，避免除本机外的机器探测到2004端口
      "protocol": "dokodemo-door",
      "settings": {
        "address": "v1.mux.cool",
        "network": "tcp",
        "followRedirect": false
      },
      "streamSettings": {
        "security": "none",
        "network": "ws",
        "wsSettings": {
          "headers": {
            "Host": ""
          },
          "path": "/SS1v2ray" //修改成自己的path
        }
      }
    },
    {
      "port": 2005,
      "protocol": "shadowsocks",
      "settings": {
        "email": "2005@gmail.com",
        "method": "chacha20-poly1305",
        "password": "diy9125", //修改成自己的密码
        "ota": false,
        "network": "tcp,udp"
      },
      "streamSettings": {
        "security": "none",
        "network": "domainsocket",
        "dsSettings": {
          "path": "/var/run/ss1-loop.sock" //与outbounds中对应
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "ss1mux",
      "protocol": "freedom",
      "streamSettings": {
        "network": "domainsocket",
        "dsSettings": {
          "path": "/var/run/ss1-loop.sock" //与streamSettings中对应
        }
      }
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "ss1ws" //与inbounds中标签对应
        ],
        "outboundTag": "ss1mux" //与outbounds中标签对应
      },
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  }
}
