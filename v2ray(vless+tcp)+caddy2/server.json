{
  "log": {
    "error": "/var/log/v2ray/error.log",
    "access": "/var/log/v2ray/access.log",
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 443, // 监听端口
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "443@gmail.com",
            "id": "048e0bf2-dd56-11e9-aa37-5600024c1d6a" //修改成自己的UUID
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 80,
            "xver": 0
          }
        ]
      },
      "streamSettings": {
        "security": "tls",
        "tlsSettings": {
          "alpn":[
            "h2",
            "http/1.1"
          ],
          "certificates": [
            {
              "certificateFile": "/etc/ssl/tls/xx.yy.crt", // 换成你的证书，绝对路径
              "keyFile": "/etc/ssl/tls/xx.yy.key" // 换成你的私钥，绝对路径
            }
          ]
        },
        "network": "tcp"
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blocked"
      }
    ]
  }
}
